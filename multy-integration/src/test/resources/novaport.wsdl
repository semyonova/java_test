</xsd:schema><xsd:schema xmlns="http://api.fabrikant.ru/multi-integration/novaport/prepareProcedure" xmlns:tns="http://api.fabrikant.ru/multi-integration/novaport/procedureInfo" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://api.fabrikant.ru/multi-integration/novaport/types" targetNamespace="http://api.fabrikant.ru/multi-integration/novaport/procedureInfo" elementFormDefault="qualified">

	<xsd:import namespace="http://api.fabrikant.ru/multi-integration/novaport/types"/>

	<xsd:complexType name="getProcedureInfo">
		<xsd:complexContent>
			<xsd:extension base="ns1:baseRequest">
				<xsd:sequence>
					<xsd:element name="PurchaseId" type="ns1:idString">
						<xsd:annotation>
							<xsd:documentation>ID процедуры во внешней системе</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="getProcedureInfoResponse">
		<xsd:complexContent>
			<xsd:extension base="ns1:baseResponse">
				<xsd:sequence>
					<xsd:element name="ProcedureUrl" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>url извещения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ProcedureNumber" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>номер извещения на ЭТП</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="RegNumberEIS" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Регистрационный номер на ЕИС</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="State" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Состояние процедуры</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LotInfoList" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Информация по лотам</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="LotInfo" type="tns:lotInfo" maxOccurs="unbounded" minOccurs="0"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="lotInfo">
		<xsd:sequence>
			<xsd:element name="LotId" type="ns1:idString">
				<xsd:annotation>
					<xsd:documentation>Идентификатор лота</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status">
				<xsd:annotation>
					<xsd:documentation>Статус лота</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="wait_publication">
							<xsd:annotation>
								<xsd:documentation>Ожидает публикацию.</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="wait_accept_proposals">
							<xsd:annotation>
								<xsd:documentation>Ожидает начало приема заявок.</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="wait_accept_proposals_draft">
							<xsd:annotation>
								<xsd:documentation>Ожидает начала приема заявок. Ожидает публикацию изменений.</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="accept_proposals">
							<xsd:annotation>
								<xsd:documentation>Идет прием заявок.</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="accept_proposals_draft">
							<xsd:annotation>
								<xsd:documentation>Идет прием заявок. Ожидает публикацию изменений.</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="evaluation">
							<xsd:annotation>
								<xsd:documentation>Идет этап оценки и сопоставления заявок.</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="finished">
							<xsd:annotation>
								<xsd:documentation>Процедура завершена.</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="cancelled">
							<xsd:annotation>
								<xsd:documentation>Процедура отменена.</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="OosStatus">
				<xsd:annotation>
					<xsd:documentation>Статус отправки на ООС</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="not_sent">
							<xsd:annotation>
								<xsd:documentation>Не отправлено на ООС</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="sent">
							<xsd:annotation>
								<xsd:documentation>Отправлено на ООС</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="published">
							<xsd:annotation>
								<xsd:documentation>Опубликовано на ООС</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="OosUrl" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>URL процедуры на ООС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OosRegNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Регистрационный номер процедуры на ООС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
