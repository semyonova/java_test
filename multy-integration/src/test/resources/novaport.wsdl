<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:ns1="http://api.fabrikant.ru/multi-integration/novaport" xmlns:ns2="http://api.fabrikant.ru/multi-integration/novaport/method" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" targetNamespace="http://api.fabrikant.ru/multi-integration/novaport">

    <wsdl:types><xsd:schema xmlns="http://api.fabrikant.ru/multi-integration/novaport/types" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xmime="http://www.w3.org/2005/05/xmlmime" targetNamespace="http://api.fabrikant.ru/multi-integration/novaport/types" elementFormDefault="qualified">

	<xsd:simpleType name="idString">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="idFile">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="notEmptyString">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="baseRequest" abstract="true"/>

	<xsd:complexType name="baseResponse" abstract="true"/>

	<xsd:complexType name="errorData">
		<xsd:sequence>
			<xsd:element name="Code" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Message" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="currency">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="RUB">
				<xsd:annotation>
					<xsd:documentation>Российский рубль</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="USD">
				<xsd:annotation>
					<xsd:documentation>Американский доллар</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EUR">
				<xsd:annotation>
					<xsd:documentation>Евро</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="GBP">
				<xsd:annotation>
					<xsd:documentation>Фунт стерлингов</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UAH">
				<xsd:annotation>
					<xsd:documentation>Украинская гривна</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="customer">
		<xsd:annotation>
			<xsd:documentation>Данные заказчика</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="INN" type="inn"/>
			<xsd:element name="OGRN" type="ogrn" minOccurs="0"/>
			<xsd:element name="KPP" type="kpp" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="inn">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{10,12}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ogrn">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{13,15}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="kpp">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{9}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="okpdCode">
		<xsd:annotation>
			<xsd:documentation>Код ОКДП</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="okvedCode">
		<xsd:annotation>
			<xsd:documentation>Код ОКВЭД</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="okeiCode">
		<xsd:annotation>
			<xsd:documentation>Код ОКЕИ</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d+"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="price">
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="0"/>
			<xsd:fractionDigits value="2"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="quantity">
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="0"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="percent">
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="100"/>
			<xsd:fractionDigits value="2"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ndsType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="nds_10">
				<xsd:annotation>
					<xsd:documentation>В том числе НДС 10%</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="nds_18">
				<xsd:annotation>
					<xsd:documentation>В том числе НДС 18%</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="without_nds">
				<xsd:annotation>
					<xsd:documentation>Без НДС</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="not_payer_nds">
				<xsd:annotation>
					<xsd:documentation>Без НДС (плательщиком НДС не является)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="not_subject_to_nds">
				<xsd:annotation>
					<xsd:documentation>НДС не облагается</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="priceVisibility">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="own">
				<xsd:annotation>
					<xsd:documentation>Только собственные предложения</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="best">
				<xsd:annotation>
					<xsd:documentation>Лучшая цена на данный момент</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="all">
				<xsd:annotation>
					<xsd:documentation>Цены всех поставщиков</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="requisites">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{1,50}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="addressType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="post">
				<xsd:annotation><xsd:documentation>Почтовый адрес</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="legal">
				<xsd:annotation><xsd:documentation>Юридический адрес</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ndsMode">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="with_nds">
				<xsd:annotation>
					<xsd:documentation>с НДС</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="without_nds">
				<xsd:annotation>
					<xsd:documentation>без НДС</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="typeInviteParticipants">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="all">
				<xsd:annotation>
					<xsd:documentation>всех</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="some">
				<xsd:annotation>
					<xsd:documentation>выборочно по списку приглашенных</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="purchaseMode">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="open">
				<xsd:annotation>
					<xsd:documentation>открытая форма</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="closed">
				<xsd:annotation>
					<xsd:documentation>закрытая форма (необходимо разослать персональные приглашения)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="LotType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="mtr">
				<xsd:annotation>
					<xsd:documentation>МТР</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="work">
				<xsd:annotation>
					<xsd:documentation>Работы</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="service">
				<xsd:annotation>
					<xsd:documentation>Услуги</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="proposalStatus">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="applicant">
				<xsd:annotation><xsd:documentation>Претендент</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="accepted">
				<xsd:annotation><xsd:documentation>Допущен организатором</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="refused">
				<xsd:annotation><xsd:documentation>Отказался от участия</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="rejected">
				<xsd:annotation><xsd:documentation>Отклонен организатором</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="lotDecisionResult">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="fail">
				<xsd:annotation>
					<xsd:documentation>Признать закупку несостоявшейся.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="fail_and_repeat">
				<xsd:annotation>
					<xsd:documentation>Признать закупку несостоявшейся и назначить повторную.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="winners">
				<xsd:annotation>
					<xsd:documentation>Назначить победителей.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="single_winner">
				<xsd:annotation>
					<xsd:documentation>Назначить единственного победителя.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="fileInstance">
		<xsd:annotation>
			<xsd:documentation>
				Файл
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FileName" minOccurs="1">
				<xsd:simpleType>
					<xsd:annotation>
						<xsd:documentation>Имя файла, включая расширение</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="250"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Title" minOccurs="1">
				<xsd:simpleType>
					<xsd:annotation>
						<xsd:documentation>Название файла</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="250"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Description" minOccurs="0">
				<xsd:simpleType>
					<xsd:annotation>
						<xsd:documentation>Описание файла</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="250"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Content" type="xsd:base64Binary" xmime:expectedContentTypes="application/octet-stream">
				<xsd:annotation>
					<xsd:documentation>Содержимое файла в base64Binary</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

</xsd:schema><xsd:schema xmlns="http://api.fabrikant.ru/multi-integration/novaport/prepareProcedure" xmlns:tns="http://api.fabrikant.ru/multi-integration/novaport/prepareProcedure" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://api.fabrikant.ru/multi-integration/novaport/types" targetNamespace="http://api.fabrikant.ru/multi-integration/novaport/prepareProcedure" elementFormDefault="qualified">

	<xsd:import namespace="http://api.fabrikant.ru/multi-integration/novaport/types"/>

	<xsd:complexType name="prepareProcedure">
		<xsd:complexContent>
			<xsd:extension base="ns1:baseRequest">
				<xsd:sequence>
					<xsd:element name="PurchaseId" type="ns1:idString">
						<xsd:annotation>
							<xsd:documentation>ID процедуры во внешней системе</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Purchase" type="tns:purchase">
						<xsd:annotation>
							<xsd:documentation>
								Данные процедуры
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="purchase">
		<xsd:annotation>
			<xsd:documentation>Процедура</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PurchaseMode" type="ns1:purchaseMode">
				<xsd:annotation>
					<xsd:documentation>Форма процедуры закупки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PurchaseName">
				<xsd:annotation>
					<xsd:documentation>Наименование процедуры</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CommonName">
				<xsd:annotation>
					<xsd:documentation>Общее наименование закупки</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="300"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SubmissionEndDate" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Дата завершения приема заявок</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReviewDate" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время рассмотрения заявок</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReviewPlace" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Место рассмотрения заявок</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="FinishDate" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Дата подведения итогов процедуры</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NotRNP" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Требовать отсутствия участников в РНП</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriceVisibility" type="ns1:priceVisibility">
				<xsd:annotation>
					<xsd:documentation>Видимость цен для участников</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeedUnsealProtocol" type="xsd:boolean" minOccurs="0" default="1">
				<xsd:annotation>
					<xsd:documentation>Необходим ли протокол вскрытия конвертов?</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsProlongationUnsealingDate" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Продление приема заявок при изменении предложения после окончания приема заявок</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Unscheduled" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Закупка осуществляется вследствие аварии (Только с интеграцией с ЕИС)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Customer" type="ns1:customer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заказчик</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CustomerContacts" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Контактная информация заказчика</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="4000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="LotList">
				<xsd:annotation>
					<xsd:documentation>Список лотов</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Lot" type="tns:purchaseLot" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="purchaseLot">
		<xsd:sequence>
			<xsd:element name="LotId" type="ns1:idString">
				<xsd:annotation>
					<xsd:documentation>Идентификатор лота</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LotType" type="ns1:LotType">
				<xsd:annotation>
					<xsd:documentation>Тип предмета закупки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name">
				<xsd:annotation>
					<xsd:documentation>Краткое описание предмета договора</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="2000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PlanInfo" type="tns:planInfo" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о плане и позиции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OkpdList">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="OkpdCode" type="ns1:okpdCode" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Предмет договора (ОКПД2)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="OkvedCode" type="ns1:okvedCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Предмет договора (ОКВЭД2)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Quantity" type="ns1:quantity">
				<xsd:annotation>
					<xsd:documentation>Количество</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Units" type="ns1:okeiCode">
				<xsd:annotation>
					<xsd:documentation>Единицы измерения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Currency" type="ns1:currency">
				<xsd:annotation>
					<xsd:documentation>Валюта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriceInfo" type="tns:priceInfo" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Начальная цена</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProposalEnsuring" type="ns1:price" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма обеспечения заявки на участие</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServiceCondition">
				<xsd:annotation>
					<xsd:documentation>Условия поставки/оказания услуг</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="2000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ServicePlace">
				<xsd:annotation>
					<xsd:documentation>Место оказания услуг</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="4000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SupplyTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сроки оказания услуг/поставки товаров</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="4000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="MspLimitation" type="mspLimitation">
				<xsd:annotation>
					<xsd:documentation>Особенности участия субъектов малого и среднего предпринимательства</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MspProvider" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>В отношении участников закупки установлено требование о привлечении к исполнению договора субподрядчиков (соисполнителей) из числа субъектов малого и среднего предпринимательства</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentConditions" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Форма, сроки и порядок оплаты</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="4000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Comment" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="4000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="priceInfo">
		<xsd:sequence>
			<xsd:element name="StartingPrice">
				<xsd:annotation>
					<xsd:documentation>Начальная цена предмета договора</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:pattern value="\d{1,13}(\.\d{1,2})?"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PricePerUnit" type="ns1:price" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Цена за единицу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NdsType" type="ns1:ndsType">
				<xsd:annotation>
					<xsd:documentation>НДС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="planInfo">
		<xsd:sequence>
			<xsd:element name="PlanRegistrationNumber">
				<xsd:annotation>
					<xsd:documentation>Регистрационный номер плана</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="\d{10}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PositionNumber" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Номер позиции плана</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="mspLimitation">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation>Нет ограничений по принадлежности к субъектам МСП</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Ограничение по принадлежности к субъектам малого предпринимательства</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>Ограничение по принадлежности к субъектам Среднего предпринимательства</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="4">
				<xsd:annotation>
					<xsd:documentation>Ограничение по принадлежности к субъектам малого и среднего предпринимательства</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="prepareProcedureResponse">
		<xsd:complexContent>
			<xsd:extension base="ns1:baseResponse">
				<xsd:choice>
					<xsd:sequence>
						<xsd:element name="ProcedureUrl" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>url извещения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ProcedureNumber" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>номер извещения на ЭТП</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>

					<xsd:element name="Error" type="ns1:errorData" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Ошибка выполнения запроса</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


</xsd:schema><xsd:schema xmlns="http://api.fabrikant.ru/multi-integration/novaport/uploadFile" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://api.fabrikant.ru/multi-integration/novaport/types" xmlns:xmime="http://www.w3.org/2005/05/xmlmime" targetNamespace="http://api.fabrikant.ru/multi-integration/novaport/uploadFile" elementFormDefault="qualified">

